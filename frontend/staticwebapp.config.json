{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/forbidden.html",
      "statusCode": 403
    }
  }
}
